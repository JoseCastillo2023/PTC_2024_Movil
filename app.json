{
  "expo": {
    "name": "cliente-app-panaderia",
    "slug": "cliente-app-panaderia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash_wallpaper.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FFFFF",
      "imageResizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],        
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash_wallpaper.png",
        "backgroundColor": "#FFF"
      }
    },
    "web": {
      "favicon": "./assets/adaptiveIcon.png"
    }
  }
}
